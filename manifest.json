{
	"name": "Duck-Duck-Snooze"
	,"short_name": "dd-snooze"
	,"version": "0.1.0"
	,"manifest_version": 2
	,"description": "Collapse unwanted search results"
	,"homepage_url": "https://github.com/clanceyp/chrome-extension-duck-duck-snooze"
	,"icons": {
		"32": "lib/i/duck.png"
		,"48": "lib/i/duck.png"
		,"128": "lib/i/duck.png"
	}
	,"content_scripts": [{
			"run_at" :"document_end",
			"matches": ["https://duckduckgo.com/*"],
			"js": ["lib/content/zepto.min.js","lib/content/content.js"]
		}
	]
}
